    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
